{
  "name": "@freetimechat/api",
  "version": "0.1.0",
  "private": true,
  "description": "FreeTimeChat Express API backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:unit": "jest __tests__/unit",
    "test:integration": "jest __tests__/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules .turbo coverage",
    "prisma:generate": "prisma generate --schema=prisma/schema-main.prisma && prisma generate --schema=prisma/schema-client.prisma",
    "prisma:generate:main": "prisma generate --schema=prisma/schema-main.prisma",
    "prisma:generate:client": "prisma generate --schema=prisma/schema-client.prisma",
    "prisma:migrate:main": "prisma migrate dev --schema=prisma/schema-main.prisma",
    "prisma:migrate:client": "prisma migrate dev --schema=prisma/schema-client.prisma",
    "prisma:migrate:deploy:main": "prisma migrate deploy --schema=prisma/schema-main.prisma",
    "prisma:migrate:deploy:client": "prisma migrate deploy --schema=prisma/schema-client.prisma",
    "prisma:studio:main": "prisma studio --schema=prisma/schema-main.prisma --browser none",
    "prisma:studio:client": "prisma studio --schema=prisma/schema-client.prisma --port 5556 --browser none",
    "prisma:format": "prisma format --schema=prisma/schema-main.prisma && prisma format --schema=prisma/schema-client.prisma",
    "prisma:validate": "prisma validate --schema=prisma/schema-main.prisma && prisma validate --schema=prisma/schema-client.prisma",
    "prisma:push:main": "prisma db push --schema=prisma/schema-main.prisma",
    "prisma:push:client": "prisma db push --schema=prisma/schema-client.prisma",
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts",
    "test:login": "API_URL=http://localhost:3001 npx tsx scripts/test-login.ts",
    "cleanup:test-data": "npx tsx scripts/cleanup-test-data.ts",
    "benchmark": "node benchmarks/run-all.js",
    "benchmark:health": "node benchmarks/health-check.js",
    "benchmark:auth": "node benchmarks/auth-login.js",
    "analyze:db": "node scripts/analyze-db-performance.js",
    "analyze:security": "node scripts/analyze-security.js",
    "analyze:authz": "node scripts/analyze-authorization.js",
    "analyze:tenant": "node scripts/analyze-multi-tenant.js",
    "analyze:all": "node scripts/analyze-db-performance.js && node scripts/analyze-security.js && node scripts/analyze-authorization.js && node scripts/analyze-multi-tenant.js"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
  },
  "dependencies": {
    "@freetimechat/types": "workspace:*",
    "@prisma/client": "^6.18.0",
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "validator": "^13.15.20",
    "xss": "^1.0.15",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.3",
    "axios": "^1.13.2",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "prisma": "^6.18.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
